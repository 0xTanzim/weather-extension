{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "name": "Idea for Extension",
  "description": "Get idea for extension",
  "version": "1.0.0",
  "manifest_version": 3,

  "action": {
    "default_popup": "ideaforExtention.html",
    "default_title": "Weather Extension"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "alarms",
    "scripting",
    "geolocation",
    "activeTab"
  ],

  "author": "Tanzim",
  "homepage_url": "https://github.com/0xTanzim/weather-extension"
}
